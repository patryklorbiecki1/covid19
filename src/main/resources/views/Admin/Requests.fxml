<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="API.Requests.RequestsController"
            prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #000000;">
    <children>
        <Label alignment="CENTER" layoutX="-2.0" layoutY="1.0" prefHeight="54.0" prefWidth="800.0" text="WNIOSKI" textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font size="17.0" />
            </font>
        </Label>
        <TableView fx:id="tab" layoutX="96.0" layoutY="55.0" prefHeight="285.0" prefWidth="604.0" style="-fx-background-color: #000000; -fx-border-color: #0c7d2c;">
            <columns>
                <TableColumn fx:id="idMessage" prefWidth="200.0" text="ID" style="-fx-background-color: #615c5c; -fx-border-color: #0c7d2c;" />
                <TableColumn fx:id="pesel" prefWidth="200.0" text="PESEL" style="-fx-background-color: #615c5c; -fx-border-color: #0c7d2c;" />
                <TableColumn fx:id="message" prefWidth="200.0" text="Wiadomość" style="-fx-background-color: #615c5c; -fx-border-color: #0c7d2c;" />
            </columns>
        </TableView>
        <Button alignment="CENTER" layoutX="240.0" layoutY="399.0" mnemonicParsing="false" onAction="#back" prefHeight="50.0" prefWidth="320.0" style="-fx-background-color: #0c7d2c;" text="Powrót" textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
