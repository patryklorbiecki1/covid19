        <font>
            <Font size="17.0" />
        </font>
    </Label>
    <Label fx:id="label2" contentDisplay="CENTER" layoutX="276.0" layoutY="185.0" text="Użytkownik o wybranym PESELu już nie żyje" textFill="#bf2323" visible="false">
        <font>
            <Font size="14.0" />
        </font>
    </Label>
    <Button layoutX="498.0" layoutY="434.0" mnemonicParsing="false" onAction="#back" prefHeight="37.0" prefWidth="240.0" style="-fx-background-color: #0c7d2c;" text="Powrót" textFill="WHITE">
        <font>
            <Font size="16.0" />
        </font>
    </Button>
</AnchorPane>
